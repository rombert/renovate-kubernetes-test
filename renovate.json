{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules" : [
   {
      "matchManagers": [
        "osgi"
      ],
      "enabled": false
    },
   {
      "matchSourceUrls": ["https://github.com/fabric8io/kubernetes-client"],
      "groupName": "Fabric8 Kubernetes and OpenShift Java Client ",
      "enabled": true
    }
  ]
}
